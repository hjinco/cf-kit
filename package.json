    "changeset:publish": "turbo build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "prettier": "^3.5.3",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@10.11.1",
  "engines": {
    "node": ">=18"
  }
}
